{
  "version": 1,
  "serviceConfiguration": {
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AMAZON_COGNITO_USER_POOLS",
      "cognitoUserPoolId": "authkalplaAuth"
    },
    "additionalAuthTypes": [
      {
        "mode": "API_KEY",
        "expirationTime": 365,
        "apiKeyExpirationDate": "2025-12-31T00:00:00.000Z",
        "keyDescription": "Public API Key for unauthenticated access"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      }
    },
    "apiName": "kalpla",
    "gqlSchemaPath": "kalpla-platform/amplify/backend/api/kalpla/schema.graphql",
    "resolverConfigPath": "kalpla-platform/amplify/backend/api/kalpla/resolvers",
    "defaultTableName": "kalpla",
    "region": "us-east-1"
  }
}
